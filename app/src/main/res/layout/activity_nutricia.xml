<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#743b8a"
    tools:context="com.rockfieldmd.andrew.demoscreens.Nutricia">

    <LinearLayout
        android:layout_width="304dp"
        android:layout_height="437dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutricia"
            android:textColor="@android:color/background_light"
            android:textSize="30sp"
            tools:layout_editor_absoluteX="140dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/button18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutrison Energy 1.5kcal"
            tools:layout_editor_absoluteX="89dp"
            tools:layout_editor_absoluteY="88dp" />

        <Button
            android:id="@+id/button19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutrison Multi Fibre 1kcal"
            tools:layout_editor_absoluteX="80dp"
            tools:layout_editor_absoluteY="192dp" />

        <Button
            android:id="@+id/button20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutrison Protein Plus 1.25kcal"
            tools:layout_editor_absoluteX="63dp"
            tools:layout_editor_absoluteY="290dp" />

        <Button
            android:id="@+id/button21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nutrison Soya 1kcal"
            tools:layout_editor_absoluteX="103dp"
            tools:layout_editor_absoluteY="376dp" />

    </LinearLayout>
</android.support.constraint.ConstraintLayout>
